{
  "configurations": [
    {
      "defines": [
        "__GNUC__",
        "__aarch64__"
      ],
      "includePath": [
        "${workspaceFolder}/extern/includes/libil2cpp/il2cpp/libil2cpp",
        "${workspaceFolder}/extern/includes/codegen/include",
        "${workspaceFolder}/extern/includes/fmt/fmt/include",
        "${workspaceFolder}/extern/includes/bs-cordl/include",
        "${workspaceFolder}/extern/includes",
        "${workspaceFolder}/include",
        "${workspaceFolder}/shared",
        "${workspaceFolder}",
        "${workspaceFolder}/**",
        "${env:VCPKG_ROOT}/installed/arm64-android/include/**",
        "${env:ANDROID_NDK_HOME}/sources/**",
        "${env:ANDROID_NDK_HOME}/toolchains/llvm/prebuilt/windows-x86_64/sysroot/usr/include",
